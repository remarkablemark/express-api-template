{
  "name": "express-api-template",
  "version": "1.0.0-alpha",
  "description": "Express API template.",
  "author": "Mark <mark@remarkablemark.org>",
  "scripts": {
    "dev": "DEBUG=express-api-template:* nodemon src/bin/www.ts",
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "yarn lint --fix",
    "lint:tsc": "tsc --noEmit",
    "postinstall": "husky install",
    "release": "standard-version --no-verify",
    "start": "NODE_ENV=production ts-node --transpile-only src/bin/www.ts"
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.4.12",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.1",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "private": true,
  "license": "MIT"
}
